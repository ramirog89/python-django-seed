[flake8]
ignore = E203, E266, E501, W503, E722
max-line-length = 120
max-complexity = 18
select = B,C,E,F,W,T4
exclude =
        migrations
        __pycache__,
        manage.py,
        base.py,
        development.py,
        production.py,
        test.py,
        __init__.py,
        src/tests,
        *_test*,
        *__init__.py,
        src/app/models.py